{
  "name": "mutation-log",
  "version": "0.1.0",
  "author": "ElectricSQL",
  "type": "module",
  "scripts": {
    "backend:down": "electric-sql stop --remove",
    "backend:start": "electric-sql start --with-postgres",
    "backend:stop": "electric-sql stop",
    "backend:up": "electric-sql start --with-postgres --detach",
    "client:generate": "electric-sql generate && patch ./src/generated/client/index.ts imports.patch && mv ./src/generated ../../packages/@livestore/electric-sync/src/",
    "db:migrate": "electric-sql with-config \"pg-migrations apply --database {{ELECTRIC_PROXY}} --directory ./db/migrations\""
  },
  "dependencies": {
    "@prisma/client": "^4.8.1",
    "electric-sql": "^0.12.1",
    "nanoid": "^5.0.7",
    "prisma": "4.8.1",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "wa-sqlite": "github:rhashimoto/wa-sqlite#semver:^0.9.11"
  },
  "devDependencies": {
    "@databases/pg-migrations": "^5.0.3",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vitejs/plugin-react": "^4.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.4.5",
    "vite": "^5.2.12"
  }
}
