{
  "name": "linearlite",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "backend:start": "npx electric-sql start --with-postgres",
    "backend:stop": "npx electric-sql stop",
    "backend:up": "npx electric-sql start --with-postgres --detach",
    "backend:down": "npx electric-sql stop --remove",
    "client:generate": "npx electric-sql generate",
    "client:watch": "npx electric-sql generate --watch",
    "db:migrate": "npx electric-sql with-config \"npx pg-migrations apply --database {{ELECTRIC_PROXY}} --directory ./backend/migrations\"",
    "db:psql": "npx electric-sql psql",
    "db:load-data": "node ./backend/load-data.js",
    "reset": "npm run backend:down && npm run backend:up && sleep 5 && npm run db:migrate && npm run client:generate",
    "reset-no-gen": "npm run backend:down && npm run backend:up && sleep 5 && npm run db:migrate",
    "reset-load": "npm run reset-no-gen && npm run db:load-data",
    "simulate": "node ./backend/simulate.js",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "vite": "vite"
  },
  "dependencies": {
    "@effect/schema": "0.64.3",
    "@firefox-devtools/react-contextmenu": "^5.1.1",
    "@headlessui/react": "^1.7.18",
    "@livestore/devtools-react": "0.0.46",
    "@livestore/livestore": "workspace:*",
    "@livestore/web": "latest",
    "@schickling/fps-meter": "^0.1.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tiptap/core": "2.1.13",
    "@tiptap/extension-placeholder": "2.1.13",
    "@tiptap/extension-table": "2.1.13",
    "@tiptap/extension-table-cell": "2.1.13",
    "@tiptap/extension-table-header": "2.1.13",
    "@tiptap/extension-table-row": "2.1.13",
    "@tiptap/pm": "2.1.13",
    "@tiptap/react": "2.1.13",
    "@tiptap/starter-kit": "2.1.13",
    "animate.css": "^4.1.1",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.10",
    "electric-sql": "^0.9.6",
    "fractional-indexing": "^3.2.0",
    "jsonwebtoken": "^9.0.2",
    "lodash.debounce": "^4.0.8",
    "nanoid": "^5.0.6",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.22.2",
    "react-toastify": "^9.1.3",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-window": "^1.8.10",
    "tiptap-markdown": "0.8.8",
    "uuid": "^9.0.1",
    "workbox-window": "^7.0.0",
    "zod": "3.21.1"
  },
  "devDependencies": {
    "@databases/pg": "^5.5.0",
    "@databases/pg-migrations": "^5.0.3",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^20.11.27",
    "@types/react": "^18.2.65",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18.2.22",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.8",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "fs-extra": "^11.2.0",
    "postcss": "^8.4.35",
    "prompt": "^1.3.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2",
