{
  "name": "@livestore/utils",
  "version": "0.3.0-dev.11",
  "type": "module",
  "sideEffects": [
    "./dist/global.js"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./cuid": {
      "types": "./dist/cuid/cuid.node.d.ts",
      "node": "./dist/cuid/cuid.node.js",
      "browser": "./dist/cuid/cuid.browser.js",
      "react-native": "./dist/cuid/cuid.browser.js"
    },
    "./nanoid": {
      "types": "./dist/nanoid/index.d.ts",
      "react-native": "./dist/nanoid/index.browser.js",
      "default": "./dist/nanoid/index.js"
    },
    "./effect": {
      "types": "./dist/effect/index.d.ts",
      "default": "./dist/effect/index.js"
    },
    "./node": {
      "types": "./dist/node/mod.d.ts",
      "default": "./dist/node/mod.js"
    },
    "./node-vitest": {
      "types": "./dist/node-vitest/mod.d.ts",
      "default": "./dist/node-vitest/mod.js"
    },
    "./node-vitest-polyfill": {
      "types": "./dist/node-vitest/polyfill.d.ts",
      "default": "./dist/node-vitest/polyfill.js"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "./cuid": [
        "./dist/cuid/cuid.node.d.ts"
      ],
      "./nanoid": [
        "./dist/nanoid/index.d.ts"
      ],
      "./effect": [
        "./dist/effect/index.d.ts"
      ],
      "./node": [
        "./dist/node/mod.d.ts"
      ],
      "./node-vitest": [
        "./dist/node-vitest/mod.d.ts"
      ],
      "./node-vitest-polyfill": [
        "./dist/node-vitest/polyfill.d.ts"
      ]
    }
  },
  "dependencies": {
    "@livestore/db-schema": "workspace:*",
    "msgpackr": "1.11.2",
    "nanoid": "5.0.9",
    "pretty-bytes": "6.1.1"
  },
  "devDependencies": {
    "@effect/cli": "^0.54.1",
    "@effect/experimental": "^0.39.1",
    "@effect/opentelemetry": "^0.42.1",
    "@effect/platform": "^0.75.1",
    "@effect/platform-browser": "^0.54.1",
    "@effect/platform-bun": "^0.55.1",
    "@effect/platform-node": "^0.71.1",
    "@effect/vitest": "^0.17.3",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.57.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.57.1",
    "@opentelemetry/sdk-metrics": "^1.30.0",
    "@opentelemetry/sdk-trace-base": "^1.30.1",
    "@opentelemetry/sdk-trace-node": "^1.30.0",
    "@types/node": "^22.10.5",
    "@types/web": "^0.0.200",
    "effect": "^3.12.7",
    "vitest": "^2.1.4"
  },
  "peerDependencies": {
    "@effect/cli": "~0.54.1",
    "@effect/experimental": "~0.39.1",
    "@effect/opentelemetry": "~0.42.1",
    "@effect/platform": "~0.75.1",
    "@effect/platform-browser": "~0.54.1",
    "@effect/platform-bun": "~0.55.1",
    "@effect/platform-node": "~0.71.1",
    "@effect/vitest": "~0.17.3",
    "@opentelemetry/api": "~1.9.0",
    "effect": "~3.12.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "react-native": "./dist/index.js",
  "scripts": {
    "test": "vitest"
  }
}
