      ]
    }
  },
  "dependencies": {
    "dprint-node": "1.0.8"
  },
  "devDependencies": {
    "drizzle-orm": "^0.39.0",
    "effect": "^3.12.7",
    "vite": "^6.0.11",
    "vitest": "^2.1.4"
  },
  "peerDependencies": {
    "effect": "~3.12.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "vitest"
  }
}
