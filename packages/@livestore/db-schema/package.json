{
  "name": "@livestore/db-schema",
  "version": "0.3.0-dev.11",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
